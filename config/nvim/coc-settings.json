{
  "coc.preferences.enableFloatHighlight": false,
  "codeLens.enable": true,
  "diagnostic.checkCurrentLine": false,
  "diagnostic.virtualText": false,
  "json.enable": true,
  "npm.binPath": "npm",
  "signature.target": "echo",
  "tsserver.enableJavascript": true,
  "tsserver.log": "verbose",
  "tsserver.npm": "npm",
  "tsserver.trace.server": "verbose",
  "yank.enableCompletion": false,
  "yank.highlight.enable": false,
  "yank.highlight.duration": 0,
  "yank.list.maxsize": 50,
  "yank.priority": 30,
  "yank.limit": 4,
  "list.normalMappings": {},
  "list.insertMappings": {},
  "languageserver": {
    "ocaml": {
      "command": "ocaml-language-server",
      "args": ["--stdio"],
      "rootPatterns": ["dune", ".merlin"],
      "filetypes": ["reason", "ocaml"],
      "trace.server": "verbose",
      "ignoredRootPaths": ["~"]
    },
    "reason": {
      "command": "reason-language-server.exe",
      "rootPatterns": ["bsconfig.json"],
      "filetypes": ["reason", "ocaml"],
      "requireRootPattern": true
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "golang": {
      "command": "go-langserver",
      "filetypes": ["go"],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  }
}
